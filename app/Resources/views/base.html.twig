<!doctype html>
<html class="no-js" lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Minigram{% endblock %}</title>
        {% block stylesheets %}

        {% endblock %}

        <link rel="stylesheet" href="{{ asset('bundles/app/css/foundation.min.css') }}" />
        <link rel="stylesheet" href="{{ asset('bundles/app/css/app.css') }}" />

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
            <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>
            <script src="{{ asset('bundles/app/js/app.js') }}"></script>

        {% endblock %}
    </body>
</html>
